{
  "name": "vue-ssr-simple-steps",
  "version": "0.1.0",
  "scripts": {
    "dev": "cross-env NODE_ENV=development nodemon ./server.js",
    "build:client": "cross-env NODE_ENV=production webpack --config ./config/webpack.client.config.js",
    "build:server": "cross-env NODE_ENV=production webpack --config ./config/webpack.server.config.js",
    "watch:client": "cross-env NODE_ENV=development webpack --config ./config/webpack.client.config.js --watch",
    "watch:server": "cross-env NODE_ENV=development webpack --config ./config/webpack.server.config.js --watch",
    "build": "npm run build:client && npm run build:server",
    "serve": "node server.js"
  },
  "dependencies": {
    "axios": "^0.19.2",
    "cookie": "^0.4.1",
    "core-js": "^3.6.5",
    "dotenv": "^8.2.0",
    "element-ui": "^2.13.2",
    "express": "^4.17.1",
    "nocache": "^2.1.0",
    "on-headers": "^1.0.2",
    "vue": "^2.6.11",
    "vue-i18n": "^8.20.0",
    "vue-meta": "^2.4.0",
    "vue-router": "^3.3.4",
    "vue-server-renderer": "^2.6.11",
    "vuex": "^3.5.1",
    "vuex-router-sync": "^5.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.11.0",
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/plugin-proposal-decorators": "^7.10.5",
    "@babel/plugin-proposal-json-strings": "^7.10.4",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-syntax-import-meta": "^7.10.4",
    "@babel/plugin-transform-runtime": "^7.11.0",
    "@babel/preset-env": "^7.11.0",
    "@babel/runtime": "^7.11.0",
    "@mdi/font": "^4.9.95",
    "autoprefixer": "^9.8.6",
    "babel-loader": "^8.0.6",
    "babel-minify-webpack-plugin": "^0.3.1",
    "compression-webpack-plugin": "^3.1.0",
    "cross-env": "^5.2.1",
    "css-loader": "^3.6.0",
    "file-loader": "^4.3.0",
    "friendly-errors-webpack-plugin": "^1.7.0",
    "html-webpack-plugin": "^3.2.0",
    "mini-css-extract-plugin": "^0.8.2",
    "node-sass": "^4.14.1",
    "nodemon": "^2.0.4",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "postcss-loader": "^3.0.0",
    "resolve-url-loader": "^3.1.1",
